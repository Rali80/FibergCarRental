@model dynamic

<h1>Login</h1>
@if (ViewBag.Error != null)
{
    <div class="alert alert-danger" role="alert">
        @ViewBag.Error
    </div>
}
<form asp-controller="Customer" asp-action="Login" method="post">
    <input type="hidden" name="carId" value="@ViewBag.CarId" />
    <div class="mb-3">
        <label for="email" class="form-label">Email</label>
        <input type="email" class="form-control" id="email" name="email" required />
    </div>
    <div class="mb-3">
        <label for="password" class="form-label">Password</label>
        <input type="password" class="form-control" id="password" name="password" required />
    </div>
    <button type="submit" class="btn btn-primary">Login</button>
</form>
<p>Don't have an account? <a asp-controller="Customer" asp-action="Register" asp-route-carId="@ViewBag.CarId">Register</a></p> <a asp-controller="Customer" asp-action="Register" asp-route-carId="@ViewBag.CarId">Register</a></p>